            pipe.geometry
           FROM (pipe_find_parent_error fp
             JOIN vw_pipe_schema_visibleitems pipe ON (((fp.id = pipe.fk_parent) AND (fp.depth_level < 20))))
        )
 SELECT foo.groupid,
