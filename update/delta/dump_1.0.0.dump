CREATE FUNCTION ft_vw_qwat_node_delete() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
	BEGIN
	CASE
		WHEN OLD.node_type::qwat_od.node_type = 'element'::qwat_od.node_type THEN
			DELETE FROM qwat_od.network_element WHERE id = OLD.id;
	END CASE;
