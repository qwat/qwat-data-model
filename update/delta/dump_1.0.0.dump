				, fk_object_reference
				, altitude
				, year
				, year_end
				, orientation
				, remark
				, schema_force_visible
				, label_1_visible
				, label_1_x
				, label_1_y
				, label_1_rotation
				, label_1_text
				, label_2_visible
				, label_2_x
				, label_2_y
				, label_2_rotation
				, label_2_text
			) VALUES (
				NEW.id
				, NEW.identification
				, NEW.fk_distributor
				, NEW.fk_status
				, NEW.fk_folder
				, NEW.fk_locationtype
				, NEW.fk_precision
				, NEW.fk_precisionalti
				, NEW.fk_object_reference
				, NEW.altitude
				, NEW.year
				, NEW.year_end
				, NEW.orientation
				, NEW.remark
				, NEW.schema_force_visible
				, NEW.label_1_visible
				, NEW.label_1_x
				, NEW.label_1_y
				, NEW.label_1_rotation
				, NEW.label_1_text
				, NEW.label_2_visible
				, NEW.label_2_x
				, NEW.label_2_y
				, NEW.label_2_rotation
				, NEW.label_2_text
		);

	 END CASE;
		RETURN NEW;
	END;
	$$;


