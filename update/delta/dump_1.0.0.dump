				update_geometry_alt2 = NEW.update_geometry_alt2
			WHERE id = NEW.id;

	CASE
		WHEN NEW.node_type::qwat_od.node_type = 'element'::qwat_od.node_type
			THEN INSERT INTO qwat_od.network_element (
				id 
				, identification
				, fk_distributor
